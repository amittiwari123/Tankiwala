<%= form_for [:admins, @distributor] do |f| %>
  <% if @distributor.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@distributor.errors.count, "error") %> prohibited this distributor from being saved:</h2>

      <ul>
      <% @distributor.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :contact %><br>
    <%= f.text_field :contact %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :city %><br>
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <%= f.label :state %><br>
    <%= f.text_field :state %>
  </div>
  <div class="field">
    <%= f.label :address %><br>
    <%= f.text_area :address %>
  </div>
  <div class="field">
    <%= f.label :gas_company_id %><br>
    <%= f.select :gas_company_id, options_for_select(GasCompany.all.collect {|p| [ p.name, p.id ] },(f.object.try(:gas_company_id))),{include_blank: 'None'}, { :class => ' input-lg'} %>
  </div>
  <div class="field">
    <%= f.label :location %><br>
    <%= f.select :location_id, options_for_select(Location.all.collect {|p| [ p.name, p.id ] },(f.object.try(:location_id))),{include_blank: 'None'}, { :class => ' input-lg'} %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
