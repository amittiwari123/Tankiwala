<p id="notice"><%= notice %></p>

<h1>Listing Distributors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Contact</th>
      <th>Email</th>
      <th>City</th>
      <th>State</th>
      <th>Address</th>
      <th>Location</th>
      <th>Gas Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distributors.each do |distributor| %>
      <tr>
        <td><%= distributor.name %></td>
        <td><%= distributor.contact %></td>
        <td><%= distributor.email %></td>
        <td><%= distributor.city %></td>
        <td><%= distributor.state %></td>
        <td><%= distributor.address %></td>
        <td><%= distributor.location.name %></td>
        <td><%= distributor.gas_company.name %></td>
        <td><%= link_to 'Show', admins_distributor_path(distributor) %></td>
        <td><%= link_to 'Edit', edit_admins_distributor_path(distributor) %></td>
        <td><%= link_to 'Destroy', admins_distributor_path(distributor), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Distributor', new_admins_distributor_path %>
